{
  "name": "imeal",
  "version": "1.0.0",
  "description": "A meal tracking",
  "main": "index.js",
  "repository": "git@github.com:austinroy/imeal.git",
  "author": "Austin Roy <austin.roy@andela.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "start": "export NODE_ENV=production && node bin/www",
    "start:dev": "nodemon --exec babel-node bin/www",
    "test": "mocha --require babel-core/register test/**/*.js --timeout 1000 --exit",
    "pretest": "node pretest.js",
    "build": "yarn clean && babel src -d dist",
    "heroku-postbuild": "yarn build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.1.3",
    "mongoose-unique-validator": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
  },
  "engines": {
    "node": "10.9.0",
    "npm": "6.2.0",
    "yarn": "1.9.4"
  }
}
